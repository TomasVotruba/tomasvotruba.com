{
    "name": "tomasvotruba/website",
    "license": "MIT",
    "description": "Blog about hacking PHP, education and being the laziest PHP programmer in the world",
    "require": {
        "php": ">=8.1",
        "erusev/parsedown-extra": "^0.8.1",
        "imagine/imagine": "^1.3",
        "laravel/framework": "^10.0",
        "nette/utils": "^3.2.9",
        "symfony/asset": "^6.2.5",
        "symfony/console": "^6.2.5",
        "symfony/debug-bundle": "^6.2",
        "symfony/framework-bundle": "^6.2.5",
        "symfony/http-kernel": "^6.2.6",
        "symfony/mime": "^6.2",
        "symfony/dotenv": "^6.2",
        "symfony/string": "^6.2",
        "symfony/twig-bundle": "^6.2",
        "symfony/yaml": "^6.2",
        "twig/twig": "^3.5.1"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^1.9.17",
        "phpstan/phpstan-webmozart-assert": "^1.2.2",
        "phpunit/phpunit": "^10.0",
        "rector/rector": "dev-main",
        "sebastian/diff": "^5.0",
        "symplify/easy-coding-standard": "^11.2",
        "symplify/package-builder": "^11.2",
        "symplify/phpstan-extensions": "^11.2",
        "tomasvotruba/cognitive-complexity": "^0.0.1",
        "tomasvotruba/type-coverage": "^0.0.1",
        "tomasvotruba/unused-public": "^0.0.32",
        "tracy/tracy": "^2.9.6"
    },
    "autoload": {
        "psr-4": {
            "TomasVotruba\\Website\\": "src",
            "TomasVotruba\\Blog\\": "packages/blog/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TomasVotruba\\Utils\\": "utils",
            "TomasVotruba\\Utils\\Tests\\": "utils-tests"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse --ansi --error-format symplify",
        "check-cs": "vendor/bin/ecs check --ansi",
        "fix-cs": [
            "vendor/bin/ecs check --fix --ansi",
            "bin/clear_posts_trailing_whitespaces.sh"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
