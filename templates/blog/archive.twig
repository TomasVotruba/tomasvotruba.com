{% extends "base.twig" %}

{% block content %}
    <div class="container-fluid" id="blog">
        <h1>{{ title }}</h1>

        {% for year, posts in posts_by_year %}
            <h2 class="text-center mt-5 mb-5" id="{{ year }}">{{ year }}</h2>
            {% include "_snippets/post/post_list.twig" with { posts: posts } %}
        {% endfor %}
    </div>
{% endblock %}
